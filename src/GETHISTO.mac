	W !, "EntrÃ©e: "_IDEB_","_IFIN_","_ANNEE_","_ANNEEFIN_","_MOISDEB_","_LEV_","_SECT_","_FAM_","_NEW_","_IDCBMSG_","_IDMSG
	s CPT=0
	k ^HISTOARTresult
	s ANNEE=$O(@FICHIER@(ANNEE))
	while ANNEE'=""
	{               
	        if (ANNEE'="")&&(ANNEE=ANNEEFIN){q}

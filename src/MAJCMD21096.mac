ROUTINE MAJCMD21096
MAJCMD21096
 S POSNUM=$G(^HWINFO("MAG"))
 K ^HWNEV("FRN","THEVENON",21096)
 S NLIGNE="0"
 F  S NLIGNE=$O(^HWNEV(POSNUM,21096,NLIGNE)) Q:(NLIGNE="")  D
 .S DATA=$G(^HWNEV(POSNUM,21096,NLIGNE))
 .S NUMCMDL=$P($P(DATA,"\",2),"*",1)
 .K ^HWARCCMD("ART",$P(DATA,"\",3),NUMCMDL)
 .K ^HWARCCMD("CDE",NUMCMDL)
 .K ^HWARCCMD("FAX",NUMCMDL)
 .Q
 K ^HWNEV(POSNUM,21096)
 Q
 
 
 
 
 
 
 
 
 
 
 
 
 
 